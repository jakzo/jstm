{
  "name": "@jstm/core",
  "version": "0.0.1",
  "description": "JavaScript Technology Manager -- automated project setup.",
  "main": "./src/index.ts",
  "keywords": [
    "project",
    "tech stack",
    "tooling",
    "configuration",
    "automation",
    "boilerplate"
  ],
  "author": "Jack Field <jack@jf.id.au>",
  "license": "MIT",
  "scripts": {
    "=== Generated Scripts (do not modify) ===": "",
    "build": "tsc -p ./tsconfig.build.json",
    "build:watch": "yarn build -w",
    "build:clean": "rimraf \"./dist\" *.tsbuildinfo",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx ./",
    "lint:prettier": "prettier -c \"./**/*{.json,.md}\"",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "release": "yarn build:clean && yarn build && changeset publish",
    "=== (end generated scripts) ===": "",
    "": "",
    "dev": "yarn test:watch",
    "prepublish": "",
    "// postinstall": "node ./scripts-dist/postinstall.js",
    "regenerate-config": "yarn build:clean && yarn build && yarn postinstall"
  },
  "husky": {
    "hooks": {
      "pre-push": "ts-node ./src/bin/pre-push.ts"
    }
  },
  "dependencies": {
    "@changesets/cli": "^2.8.0",
    "@jest/types": "^26.0.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/experimental-utils": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^9.0.1",
    "golden-fleece": "^1.0.9",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/fs-extra": "^9.0.1",
    "@types/prettier": "^2.0.1",
    "chalk": "^4.0.0",
    "ts-node": "^8.10.2",
    "type-fest": "^0.15.0"
  },
  "repository": "git@github.com:jakzo/jstm.git",
  "homepage": "https://github.com/jakzo/jstm"
}

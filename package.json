{
  "name": "@jakzo/project",
  "version": "0.0.1",
  "description": "Javascript project boilerplate in a box.",
  "main": "./dist/index.js",
  "keywords": [
    "project",
    "boilerplate"
  ],
  "author": "Jack Field <jack@jf.id.au>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx ./",
    "lint:prettier": "prettier -c \"./**/*{.json,.md}\"",
    "test": "yarn lint && yarn test:jest",
    "test:jest": "jest",
    "example": "cd ./__example__ && ts-node -P ../tsconfig.json ../src/bin/project.ts",
    "postinstall": "node ./dist/bin/project.js",
    "setup": "node ./scripts/setup.js"
  },
  "dependencies": {
    "@jest/types": "^26.0.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/experimental-utils": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^9.0.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/fs-extra": "^9.0.1",
    "@types/prettier": "^2.0.1",
    "ts-node": "^8.10.2"
  },
  "repository": "git@bitbucket.org:atlassian/incredible-monorepo.git",
  "homepage": "https://bitbucket.org/atlassian/incredible-monorepo/src/master/src/packages/standalone/ts-dev"
}
